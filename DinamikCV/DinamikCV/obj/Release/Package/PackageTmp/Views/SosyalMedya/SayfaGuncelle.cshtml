@model DinamikCV.Models.Entity.tblSosyalMedya
@{
    ViewBag.Title = "SayfaGuncelle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>SayfaGuncelle</h2>
<form method="post">

    @Html.Label("Ad")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("ikon")
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />
    @Html.Label("Durum")
    @Html.TextBoxFor(x => x.Durum, new { @class = "form-control" })
    <br />

    <button class="btn btn-info"> Guncelle </button>

</form>
